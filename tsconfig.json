{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "baseUrl": "/opt/gnuband/src",
    "paths": {
      "~util/*": [
        "util/*"
      ],
      "~components/*": [
        "components/*"
      ],
      "~content/*": [
        "content/*"
      ],
      "~assets/*": [
        "assets/*"
      ],
      "~layouts/*": [
        "layouts/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}